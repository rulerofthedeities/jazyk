{
  "name": "jazyk",
  "description": "Language learning tool",
  "version": "2.1.7",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "start:local": "pm2 start km-jazyk.js --watch server",
    "start:heroku": "npm run serve:ssr",
    "build:watch": "ng build --watch",
    "build:prod": "ng build --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "test:ssr": "node --inspect dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run jazyk:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "babel:ngx-utils": "node node_modules/babel-cli/bin/babel.js node_modules/@ngx-utils/cookies --out-dir node_modules/@ngx-utils/cookies --presets es2015",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build:ssr"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "6.12.0"
  },
  "author": "Kristiaan Van Ermengem",
  "private": true,
